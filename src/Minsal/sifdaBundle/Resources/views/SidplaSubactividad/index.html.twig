{% extends '::base.html.twig' %}

{% block body -%}
    <h1>SidplaSubactividad list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Descripcion</th>
                <th>Codigo</th>
                <th>Activo</th>
                <th>Metaanual</th>
                <th>Descripcionmetaanual</th>
                <th>Indicador</th>
                <th>Medioverificacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('sidplasubactividad_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.descripcion }}</td>
                <td>{{ entity.codigo }}</td>
                <td>{{ entity.activo }}</td>
                <td>{{ entity.metaAnual }}</td>
                <td>{{ entity.descripcionMetaAnual }}</td>
                <td>{{ entity.indicador }}</td>
                <td>{{ entity.medioVerificacion }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('sidplasubactividad_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('sidplasubactividad_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('sidplasubactividad_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
