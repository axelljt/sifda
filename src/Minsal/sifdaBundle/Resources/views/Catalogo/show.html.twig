{#{% extends '::base.html.twig' %}#}
{% extends 'MinsalsifdaBundle::layoutAdministrador.html.twig' %}

{% block contenido_in -%}
    <h1>Catalogo</h1>

    <table class="table table-bordered table-hover table-condensed">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Sistema</th>
                <td>{{ entity.sistema }}</td>
            </tr>
            <tr>
                <th>Ref1</th>
                <td>{{ entity.ref1 }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('sifda_catalogo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('sifda_catalogo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
