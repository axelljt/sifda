{% spaceless %}
<div id="school-previous">
    <div class="bs-example">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Descripcion</th>
                    <th>Fecha de recepcion</th>
                    <th>Codigo</th>
                </tr>
            </thead>
            <tbody>{%for o in ordenes%}
                <tr>
                    <td>{{o.descripcion}}</td>
                    <td>{{o.fechaCreacion|date('Y-m-d H:i:s')}}</td>
                    <td>{{o.codigo}}</td>
                </tr>
                   {%endfor%}
            </tbody>
        </table>
    </div>
</div>        
{% endspaceless %}
